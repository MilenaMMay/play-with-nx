{
  "version": 1,
  "projects": {
    "nx-playground": {
      "root": "apps/nx-playground",
      "sourceRoot": "apps/nx-playground/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/nx-playground",
            "index": "apps/nx-playground/src/index.html",
            "main": "apps/nx-playground/src/main.tsx",
            "polyfills": "apps/nx-playground/src/polyfills.ts",
            "tsConfig": "apps/nx-playground/tsconfig.app.json",
            "assets": [
              "apps/nx-playground/src/favicon.ico",
              "apps/nx-playground/src/assets"
            ],
            "styles": ["apps/nx-playground/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/nx-playground/src/environments/environment.ts",
                  "with": "apps/nx-playground/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "nx-playground:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "nx-playground:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/nx-playground/tsconfig.app.json",
              "apps/nx-playground/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/nx-playground/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/nx-playground/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nx-playground-e2e": {
      "root": "apps/nx-playground-e2e",
      "sourceRoot": "apps/nx-playground-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/nx-playground-e2e/cypress.json",
            "tsConfig": "apps/nx-playground-e2e/tsconfig.e2e.json",
            "devServerTarget": "nx-playground:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "nx-playground:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": ["apps/nx-playground-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/nx-playground-e2e/**/*"]
          }
        }
      }
    },
    "common": {
      "root": "libs/common",
      "sourceRoot": "libs/common/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/common/tsconfig.lib.json",
              "libs/common/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/common/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/common/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "the-other-app": {
      "root": "apps/the-other-app",
      "sourceRoot": "apps/the-other-app/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/the-other-app",
            "index": "apps/the-other-app/src/index.html",
            "main": "apps/the-other-app/src/main.tsx",
            "polyfills": "apps/the-other-app/src/polyfills.ts",
            "tsConfig": "apps/the-other-app/tsconfig.app.json",
            "assets": [
              "apps/the-other-app/src/favicon.ico",
              "apps/the-other-app/src/assets"
            ],
            "styles": ["apps/the-other-app/src/styles.css"],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/the-other-app/src/environments/environment.ts",
                  "with": "apps/the-other-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "the-other-app:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "the-other-app:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
